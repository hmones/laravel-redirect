{
    "name": "hmones/laravel-redirect",
    "description": "Redirects users automatically after login to the previous protected route they were trying to access",
    "license": "MIT",
    "authors": [
        {
            "name": "Haytahm Mones",
            "email": "haytham.mones@gmail.com",
            "homepage": "https://www.haythammones.com"
        }
    ],
    "homepage": "https://github.com/hmones/laravel-redirect",
    "keywords": ["Laravel", "LaravelRedirect"],
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "ext-json": "*",
        "composer-runtime-api": "^2.0",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Hmones\\LaravelRedirect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hmones\\LaravelRedirect\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hmones\\LaravelRedirect\\LaravelRedirectServiceProvider"
            ],
            "aliases": {
                "LaravelRedirect": "Hmones\\LaravelRedirect\\Facades\\LaravelRedirect"
            }
        }
    }
}
